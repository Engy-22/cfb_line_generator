<br/>
<% if current_user %>
<table id="TeamTable" class="tablesorter">
<thead>
<tr>
  <th> Team </th>
  <th> Conference </th>
  <th> Profile </th>
</tr>
</thead>
<tbody>
  <% @teams.each do |team| %>
  <tr>
    <td> <%= team.name %> </td>
    <td> <%= team.conference %> </td>
    <td> <% if team.has_profile?(current_user) %>
          <% team.profiles.each do |profile| %>
            <% if profile.user_id == current_user.id %>
              <%= link_to "Profile", edit_team_profile_path(profile.team, profile) %>
            <% end %>
          <% end %>
        <% else %>
          <%= link_to "Profile", new_team_profile_path(team) %>
        <% end %> </td>
    </tr>
  <% end %>
</tbody>
</table>
<% else %>
Sign Up, yo.
<% end %>
